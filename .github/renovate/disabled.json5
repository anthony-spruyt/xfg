{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Disable kubernetes-api datasource (noisy, not useful)",
      "matchManagers": [
        "kubernetes"
      ],
      "matchDatasources": [
        "kubernetes-api"
      ],
      "enabled": false
    }
  ]
}
